{
  "name": "@moonsama/minecraft-oracle-api",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:deps:up": "docker-compose -f docker-compose.deps.yml up",
    "docker:deps:up:d": "docker-compose -f docker-compose.deps.yml up -d",
    "docker:deps:down": "docker-compose -f docker-compose.deps.yml down",
    "docker:deps:init": "yarn docker:deps:up && yarn typeorm:ci:create && yarn typeorm:ci:run",
    "docker:deps:restart": "yarn docker:deps:down && yarn docker:deps:up",
    "typeorm": "node --require ts-node/register ./node_modules/.bin/typeorm",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:create": "yarn typeorm migration:create -d migrations/ -n mig",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@kiltprotocol/sdk-js": "0.28",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.6",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/typeorm": "8.0.3",
    "@polkadot/util-crypto": "^9.4.1",
    "@types/mailgun-js": "^0.22.12",
    "@types/node-fetch": "2",
    "async-mutex": "^0.3.2",
    "atob": "2.1.2",
    "aws-sdk": "^2.1143.0",
    "axios": "^0.24.0",
    "bip39": "3.0.4",
    "bn.js": "5.2.0",
    "bs58": "4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "cli-color": "^2.0.0",
    "dotenv": "^10.0.0",
    "elliptic": "6.5.4",
    "ethers": "5.4.7",
    "fast-safe-stringify": "^2.1.1",
    "file-type": "16",
    "fp-ts": "^2.11.4",
    "http-client-methods": "^1.0.3",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "ioredis": "^4.27.11",
    "joi": "^17.4.2",
    "js-sha3": "^0.8.0",
    "jwt-decode": "^3.1.2",
    "logform": "^2.3.0",
    "mailgun.js": "^7.0.2",
    "nest-winston": "^1.6.1",
    "node-fetch": "2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sharp": "^0.30.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.45",
    "winston": "^3.3.3",
    "xss": "^1.0.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/atob": "2.1.2",
    "@types/bs58": "4.0.1",
    "@types/cli-color": "2.0.1",
    "@types/elliptic": "6.4.13",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.27.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.30.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.10.1",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
